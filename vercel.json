{
    "buildCommand": "next build",
    "devCommand": "next dev",
    "framework": "nextjs",
    "public": false,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}
